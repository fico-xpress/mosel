model "testing myxprs"
 uses "mmjobs", "mmsystem"
 options keepassert

 declarations
  m: Model
 end-declarations

 forall(f in ["myxprsmodel1","myxprsmodel1p","myxprsmodel2","myxprsmodel3"]) do
   writeln("-"*30,f,"-"*30)
   fflush
   res:=compile(f+".mos")
   assert(res=0, "Compilation error for "+f)
   load(m,f+".bim")
   run(m)
   wait
   dropnextevent
 end-do
 
end-model
